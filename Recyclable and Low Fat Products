-- Create Table
CREATE TABLE products (
    product_id INT PRIMARY KEY,
    low_fats ENUM('Y', 'N'),
    recyclable ENUM('Y', 'N')
);

-- Insert Data
INSERT INTO products (product_id, low_fats, recyclable) 
VALUES 
(0, 'Y', 'N'),
(1, 'Y', 'Y'),
(2, 'N', 'Y'),
(3, 'Y', 'Y'),
(4, 'N', 'N');

select product_id from products where low_fats='Y' and  recyclable='Y'
